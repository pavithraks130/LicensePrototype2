@model System.Data.DataTable
@using System.Data;

<h5>Please enter CSV file location:</h5>
@using (Html.BeginForm("Upload", "FileAction", null, FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary()

    <div class="form-group">
        <input type="url" id="dataFile" name="link" />
    </div>
    <div class="form-group">
        <label id="lblError" style="color:green">@TempData["msg"]</label>
    </div>
    <div class="form-group">
        <input type="submit" value="Upload" class="btn btn-default" />
    </div>
   
}

